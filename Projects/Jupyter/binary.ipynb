    "def convertToBin(n):\n",
    "    max = 0\n",
    "    # binStr = str(bin(n))[2:]\n",
    "    bin_l = str(bin(n))[2:].split('0')\n",
    "    for i in bin_l:\n",
    "        if len(i) > max:\n",
    "            max = len(i)\n",
    "    return max"
   ]
